<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/stylemodal.css">
        <link href="https://unpkg.com/tailwindcss@^2/dist/tailwind.min.css" rel="stylesheet"> 

    <title>Interview Prep Tracker</title>
</head>
<style>
  body{
      margin: 0px;
      padding: 0px;
      background: url(/back.png);
  }
</style>
<body>
    <header class="text-gray-400 bg-gray-900 body-font">
        <div class="container mx-auto flex flex-wrap p-5 flex-col md:flex-row items-center">
          <a class="flex title-font font-medium items-center text-white mb-4 md:mb-0">
            <svg xmlns="http://www.w3.org/2000/svg" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" class="w-10 h-10 text-white p-2 bg-indigo-500 rounded-full" viewBox="0 0 24 24">
              <path d="M12 2L2 7l10 5 10-5-10-5zM2 17l10 5 10-5M2 12l10 5 10-5"></path>
            </svg>
            <span class="ml-3 text-xl">INTERVIEW TRACKER</span>
          </a>
          <nav class="md:ml-auto flex flex-wrap items-center text-base justify-center">
            <% if(user){ %>
              <a class="mr-5 hover:text-white">Welcome, <%= user.email %></a>
              <% if(isAdmin){ %>
                  <a href="/admin" class="mr-5 hover:text-white">ADMIN PAGE</a>
              <% } %>
              <a href="/" class="mr-5 hover:text-white">HOME</a>
              <a href="/logout" class="mr-5 hover:text-white">LOG OUT</a>
                    
              <% }else{ %>
              <a href="/" class="mr-5 hover:text-white">HOME</a>
              <a href="/login" class="mr-5 hover:text-white">LOG IN</a>
              <a href="/signup" class="mr-5 hover:text-white">SIGN UP</a>
            <% } %>
          </nav>
        </div>
      </header>
      
      <section class="text-gray-600 body-font">
        <div class="container px-5 py-24 mx-auto">
          <div class="flex flex-wrap -m-4">
            <section class="text-gray-400 body-font">
              <div class="container px-5 py-24 mx-auto">
                <div class="flex flex-wrap -m-4">
                  
                  <% list2.forEach((experience) => { %>
                    <% if(experience.approv){ %>

                    <div class="p-4 md:w-1/3">
                      <div class="h-full border-2 border-gray-800 rounded-lg overflow-hidden">
                        <% if(experience.img.data != null && experience.img.contentType != null ){ %>
                          <img class="lg:h-48 md:h-36 w-full object-cover object-center" src="data:<%=experience.img.contentType%>;base64,<%= experience.img.data.toString('base64')%>" alt="blog">
                          <% }else{ %>
                            <img class="lg:h-48 md:h-36 w-full object-cover object-center" src="https://dummyimage.com/720x400" alt="blog">
                            <% } %>
                          
                        <div class="p-6">
                          <h1 class="title-font text-lg font-medium text-white mb-3"><%= experience.name %></h1>
                          <p class="leading-relaxed text-white mb-3"><%= experience.experience.substr(0,95) %>.......</p>
                          <div class="flex items-center flex-wrap ">
                            <a class="text-indigo-400 inline-flex items-center md:mb-2 lg:mb-0" href="/company/experience/<%= experience._id %>">Learn More
                              <svg class="w-4 h-4 ml-2" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                <path d="M5 12h14"></path>
                                <path d="M12 5l7 7-7 7"></path>
                              </svg>
                            </a>
                            <span class="text-gray-500 mr-3 inline-flex items-center lg:ml-auto md:ml-0 ml-auto leading-none text-sm pr-3 py-1 border-r-2 border-gray-800">
                              </svg><%= experience.branch %>
                            </span>
                            <span class="text-gray-500 inline-flex items-center leading-none text-sm">
                              <%= experience.year %>
                            </span>
                          </div>
                        </div>
                      </div>
                    </div>
                    <% } %> 
                    <% }) %>
                  </div>
                </div>
              </section>
   
    

 <%- include('../partials/footer'); -%>